{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/events.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/bookings.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/events(.*)",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/bookings(.*)",
      "dest": "/api/bookings.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "functions": {
    "api/events.js": {
      "runtime": "nodejs18.x"
    },
    "api/bookings.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}